#ifndef TESTS_HPP_
#define TESTS_HPP_
#include<memory>

class Tests : public std::enable_shared_from_this<Tests>
{
	int a=5;
	public:
	 std::shared_ptr<Tests> getptr() {
	         return shared_from_this();
	     }

	 int getA(){return a};
};




#endif /* TESTS_HPP_ */
