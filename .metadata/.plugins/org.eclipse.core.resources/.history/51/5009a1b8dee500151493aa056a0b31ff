#ifndef TESTS_HPP_
#define TESTS_HPP_
#include<memory>

class Tests
{
	int a,b;
	char* ca = new char[5];
	public:

	 int getA(){return a;};
	 char giveCharAt(int i)
	 {
		if (i<b) return ca[i];
		return '-1';
	 }
	 Tests(int a,int b)
	 : a(a)
	 , b(b)
	{ }
	 ~Tests(){
	 }

};




#endif /* TESTS_HPP_ */
