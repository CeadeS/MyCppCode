#include "Tests.hpp"
#include <memory>
#include <stdio.h>
int main(int argc, char **argv) {
	std::shared_ptr<Tests> foo = std::make_shared< Tests >(5,5);
	//foo.reset();
	foo-> setCharAt(2,'Z');
	&(foo->getCharRefAt(3))='@';
	printf("%d,\n Array an Stelle 2 %c \n an Stelle 3 %c",foo->getA(),foo->getCharAt(2),foo->getCharAt(3));
	foo.reset();



	return -1;
}




